<div class="modal fade" id="modal-container-finalInvoice" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">

                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    Ã—
                </button>
                <h4 class="modal-title" id="myModalLabel">
                    Invoice
                </h4>
            </div>
            <div class="modal-body">
                <form role="form" id="finalInvoiceForm">
                    <div class="form-group">
                        <label>
                            User Name
                        </label>
                        <input type="text"  class="form-control" name="username" id="username" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Car Name
                        </label>
                        <input type="text"  class="form-control" name="carname" id="carname" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Pickup Date
                        </label>
                        <input type="text"  class="form-control" name="pickupDate" id="pickupDate" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Pickup Time
                        </label>
                        <input type="text"  class="form-control" name="pickUpTime" id="pickUpTime" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            DropOff Date
                        </label>
                        <input type="text"  class="form-control" name="dropOffDate" id="dropOffDate" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            DropOff Time
                        </label>
                        <input type="text"  class="form-control" name="dropOffTime" id="dropOffTime" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Amount
                        </label>
                        <input type="text"  class="form-control" name="amount" id="amount" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Discount
                        </label>
                        <input type="text"  class="form-control" name="discount" id="discount" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Total Final Amount
                        </label>
                        <input type="text"  class="form-control" name="totalAmount" id="totalAmount" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Distance Travelled
                        </label>
                        <input type="text"  class="form-control" name="distance" id="distance" disabled/>
                    </div>
                    <div class="form-group">
                        <label>
                            Damage
                        </label>
                        <input type="text"  class="form-control" name="damage" id="damage"  disabled/>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">
                            Cancel
                        </button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">
                            OK
                        </button>
                    </div>
                </form>
            </div>

        </div>

    </div>

</div>